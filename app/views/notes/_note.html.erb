<% if user_signed_in? %>
  <div class="card">
    <div class="card__body">
    <p class="card__summary"><%= note.title %></p>
      <div class="card__title"></div>
        <%= link_to "#{note.list_name}", note_lists_path(note.character), class: :card__title%>
        <%= link_to "#{note.item_name}", note_items_path(note.character), class: :card__title%>
        <%= link_to "#{note.log_name}", note_logs_path(note.character), class: :card__title%>
        <%= link_to "#{note.reserve_name}", root_path(note.character), class: :card__title%>
      <%= link_to "所有者 #{current_user.nickname}", root_path, class: :card__user %>
    </div>
  </div>
<% end %>