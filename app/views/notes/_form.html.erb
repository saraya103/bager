<%= form_with model: @note, local: true do |f|%>
  <div class="field">
    <%= f.label :genre_id, "ジャンル" %><br />
    <%= f.text_field :genre_id, id:"note_genre" %>
  </div>
  <div class="field">
    <%= f.label :list_name, "リストのタイトル" %><br />
    <%= f.text_field :list_name, id:"note_list" %>
  </div>

  <div class="field">
    <%= f.label :loge_name, "履歴のタイトル" %><br />
    <%= f.text_area :log_name, class: :form__text, id:"note_log" %>
  </div>

  <div class="field">
    <%= f.label :hoge, "コンセプト" %><br />
    <%= f.text_area :hoge, class: :form__text, id:"prototype_concept" %>
  </div>

  <div class="field">
    <%= f.label :hoge, "ノートの画像" %><br />
    <%= f.file_field :hoge, id:"prototype_image" %>
  </div>

  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>