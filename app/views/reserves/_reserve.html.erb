<tr>
  <td><%= check_box "itemcheck", "#{list.item.id}" %></td>
  <td><% if reserve.item.image.attached? %>
        <%= link_to image_tag(reserve.item.image, class: :card__img ), root_path  %>
      <% else %>
        <%= image_tag "noimg.png" %>
      <% end %></td>
  <td><%= link_to "#{reserve.item.name}", root_path, class: :card__title%></td>
  <td><%= link_to "#{reserve.item.price}", root_path, class: :card__title%></td>
  <td><%= link_to "#{reserve.item.count}", root_path, class: :card__title%></td>
  <td><%= link_to "#{reserve.item.condition}", root_path, class: :card__title%></td>
  <td><%= link_to "#{reserve.item.memo}", root_path, class: :card__title%></td>
<%#  <td><%= link_to '詳細', note_list_path(id: list.id) %><%# </td> %>
  <td><%= link_to '完了', note_reserve_path(id: reserve.id), method: :patch %></td>
  <td><%= link_to '取消', note_reserve_path(id: reserve.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
<tr>
